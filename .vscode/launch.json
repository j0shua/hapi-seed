{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/rejoice/bin/rejoice",
			"stopOnEntry": false,
			"args": ["-c", "manifest.js", "-p", "${workspaceRoot}"],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"console": "integratedTerminal",
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "local",
				"PORT": "9999"
			},
			"sourceMaps": false
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5855,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outDir": null,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}
